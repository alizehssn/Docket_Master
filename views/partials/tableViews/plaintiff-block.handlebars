<li>
    <p>{{FName}} {{LName}}</p>

    <form>
        <div class="list-group-item">
            <b>PLAINTIFF FIRST NAME:</b> <input id="plaintiffFName" placeholder="{{FName}}"></input>
        </div>
        <div class="list-group-item">
            <b>PLAINTIFF LAST NAME:</b> <input id="plaintiffLName" placeholder="{{LName}}"></input>
        </div>
        <div class="modal-footer">
            <button type="button" class="btn btn-secondary" id="update-plaintiff" data-dismiss="modal"
                data-id="{{id}}">Update
                Plaintiff</button>
        </div>
    </form>
</li>