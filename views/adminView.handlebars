<div class="card">
    <div class="card-header">
        Plaintiffs
    </div>
    <div class="card-body">
        <ol>
            {{#each plaintiffs}}
            {{> tableViews/plaintiff-block}}
            <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#Modal{{@index}}">Update
                Plaintiff</button>

            <div class="modal fade" id="Modal{{@index}}" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
                aria-hidden="true">
                <div class="modal-dialog" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title" id="exampleModalLabel">Plaintiff</h5>
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                        </div>
                        <div class="modal-body">
                            <ul class="list-group">
                                <li class="list-group-item">
                                    <b>PLAINTIFF FIRST NAME:</b> <input id="plaintiffFName" placeholder="{{this.FName}}"></input>
                                </li>
                                <li class="list-group-item">
                                    <b>PLAINTIFF LAST NAME:</b> <input id="plaintiffLName" placeholder="{{this.LName}}"></input>
                            </ul>
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-secondary" id="update-plaintiff"data-dismiss="modal" data-id="{{id}}">Update
                                Plaintiff</button>
                        </div>
                    </div>
                </div>
            </div>
            {{/each}}
        </ol>
    </div>
</div>
<div class="modal fade" id="Modal{{@index}}" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
    aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Plaintiff</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <ul class="list-group">
                    <li class="list-group-item">
                        <b>PLAINTIFF FIRST NAME:</b> <input placeholder={{this.Plaintiff.FName}}></input>
                    </li>
                    <li class="list-group-item">
                        <b>PLAINTIFF LAST NAME:</b> <input placeholder="{{this.PlaintiffAttorney.LName}}"></input>
                </ul>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Update Plaintiff</button>
            </div>
        </div>
    </div>
</div>

